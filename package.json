{
  "name": "actual-mcp",
  "version": "0.2.4",
  "description": "MCP server for Actual Budget API",
  "type": "module",
  "main": "dist/index.js",
  "bin": {
    "actual-mcp": "./dist/index.js"
  },
  "scripts": {
    "extract-api": "npx tsx scripts/extract-api-methods.ts",
    "extract-aql-schema": "npx tsx scripts/extract-aql-schema.ts",
    "generate-descriptions": "npm run extract-api && npx tsx scripts/generate-api-method-descriptions.ts",
    "check-descriptions": "npm run extract-api && npx tsx scripts/generate-api-method-descriptions.ts --check",
    "generate-manifest": "npm run extract-api && npx tsx scripts/generate-api-method-descriptions.ts && npx tsx scripts/generate-manifest.ts",
    "check-manifest": "npm run extract-api && npx tsx scripts/generate-api-method-descriptions.ts --check && npx tsx scripts/generate-manifest.ts --check",
    "build": "tsc",
    "prepare": "npm run build && husky",
    "start": "node dist/index.js",
    "dev": "tsx watch src/index.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mweichert/actual-mcp.git"
  },
  "keywords": [
    "mcp",
    "actual-budget",
    "cli",
    "model-context-protocol"
  ],
  "author": "mweichert",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mweichert/actual-mcp/issues"
  },
  "homepage": "https://github.com/mweichert/actual-mcp#readme",
  "dependencies": {
    "@actual-app/api": "https://github.com/mweichert/actual/releases/download/v26.1.0-fork.1/actual-app-api.tgz",
    "@modelcontextprotocol/sdk": "^1.0.0",
    "zod": "^3.25.0"
  },
  "devDependencies": {
    "@types/node": "^22.0.0",
    "husky": "^9.1.7",
    "ts-morph": "^27.0.2",
    "tsx": "^4.0.0",
    "typescript": "^5.7.0"
  },
  "engines": {
    "node": ">=20"
  }
}
